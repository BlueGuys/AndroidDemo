<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hongyan.androiddemo">

    <application
        android:name="com.hongyan.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/apple"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hongyan.ui.UIActivity" />
        <activity android:name="com.hongyan.ui.UICommonActivity" />
        <activity android:name="com.hongyan.ui.UIShapeActivity" />
        <activity android:name="com.hongyan.ui.UIBitmapActivity" />
        <activity android:name="com.hongyan.base.BaseWebViewActivity" />
    </application>

</manifest>